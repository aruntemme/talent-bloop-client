  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "setup": "npm i",
    "lint:js": "./node_modules/.bin/eslint \"src/**/*.js\"",
    "lint:styles": "stylelint \"src/**/*.css\"",
    "start": "npm run preImageHook && cross-env NODE_ENV=development --VARIANT=local webpack-dev-server --config ./config/webpack.config.js",
    "start:dist": "npm run preImageHook && cross-env NODE_ENV=production --VARIANT=local webpack-dev-server --config ./config/webpack.config.js && http-server ./dist -o",
    "build:dist": "npm run preImageHook && cross-env NODE_ENV=production --VARIANT=local webpack --config ./config/webpack.config.js",
    "build:dist:local": "npm run preImageHook && cross-env NODE_ENV=production --VARIANT=local webpack --config ./config/webpack.config.js",
    "build:dist:dev-live": "npm run preImageHook && cross-env NODE_ENV=production --VARIANT=dev-live webpack --config ./config/webpack.config.js",
    "build:dist:live": "npm run preImageHook && cross-env NODE_ENV=production --VARIANT=live webpack --config ./config/webpack.config.js && npm run postBuildHook",
    "analyse": "npm run preImageHook && cross-env NODE_ENV=production --VARIANT=local webpack --config ./config/webpack.config.js --profile --json > dist/build/stats.json && webpack-bundle-analyzer dist/build/stats.json",
    "generateWebp": "node ./config/webpConverter.js",
    "resizeImages": "node ./config/imageResizer.js",
    "preImageHook": "npm run generateWebp && npm run resizeImages",
    "postBuildHook": "node ./helper.js"
  },
